<%include ../partials/header.ejs %>

<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <h1><%= title %></h1>
      
      <a class="btn btn-primary" href="/health-list/add" role="button">
        <i class="fa fa-plus-circle"> Add a Contact </i>
      </a>

      <table class="table">
        <thead class="thead-light">
          <tr>
            <th scope="col">patients</th>
          
          </tr>
        </thead>

        <% for (let count = 0; count < data.length; count++ ){ %>

        <tbody>
          <tr>
            <td><%= data[count] %></td>
          </tr>
        </tbody>
        <% } %>
      </table>
    </div>
  </div>
</div>

<%include ../partials/footer.ejs %>
