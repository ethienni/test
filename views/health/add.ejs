<% include ../partials/header.ejs %>

<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <h1><%= title %></h1>

      <form class="form" method="POST">
        <div class="form-group">
          <label for="caregiver">Care Giver</label
          ><input
            type="text"
            class="form-control"
            id="caregiverTextField"
            name="caregiver"
            required
          />
        </div>

        <div class="form-group">
          <label for="patient">Patient</label
          ><input
            type="text"
            class="form-control"
            id="patientTXT"
            name="patient"
            required
          />
        </div>

        <div class="form-group">
          <label for="behaviour">behaviour</label
          ><input
            type="text"
            class="form-control"
            id="behaviourTXT"
            name="behaviour"
            required
          />
        </div>

        <div class="form-group">
          <label for="interventiondone">interventiondone</label
          ><input
            type="text"
            class="form-control"
            id="interventiondoneTXT"
            name="interventiondone"
            required
          />
        </div>

        <div class="form-group">
          <label for="behaviour">dateandtime</label
          ><input
            type="text"
            class="form-control"
            id="dateandtimeTXT"
            name="dateandtime"
            required
          />
        </div>


        <div class="form-group">
          <label for="location">location</label
          ><input
            type="text"
            class="form-control"
            id="locationTXT"
            name="location"
            required
          />
        </div>


        <div class="form-group">
          <label for="comments">Comments</label
          ><input
            type="text"
            class="form-control"
            id="CommentsTXT"
            name="Comments"
            required
          />
        </div>


        
        
        <button type="submit" class="btn btn-primary">
          <i class="fa fa-edit    "></i>
          Submit
        </button>
        <a href="/health-list" class="btn btn-warning">
          <i class="fa fa-undo" aria-hidden="true"> Cancel </i>
        </a>
      </form>
    </div>
  </div>
</div>

<% include ../partials/footer.ejs %>
